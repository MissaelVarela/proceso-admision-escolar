        "../{{context.solicitud_ref.link}}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.solicitud && $data.solicitud_filtros && $data.solicitud_informacion && $data.solicitud_informacion_datos_solicitud && $data.solicitud_informacion_datos_personales && $data.solicitud_informacion_datos_escuela_cursada && $data.solicitud_informacion_datos_familiares ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.solicitud.filtros = $data.solicitud_filtros;",
        "\t$data.solicitud.informacion = $data.solicitud_informacion;",
        "\t$data.solicitud.informacion.datos_solicitud = $data.solicitud_informacion_datos_solicitud;",
        "\t$data.solicitud.informacion.datos_personales = $data.solicitud_informacion_datos_personales;",
        "\t$data.solicitud.informacion.datos_escuela_cursada = $data.solicitud_informacion_datos_escuela_cursada;",
        "\t$data.solicitud.informacion.datos_familiares = $data.solicitud_informacion_datos_familiares;",
        "\treturn {",
        "\t\t//map solicitud variable to expected task contract input",
        "\t\tsolicitudInput: {",
        "\t\t\tinformacion: $data.solicitud.informacion ? {",
        "\t\t\t\tdatos_familiares: $data.solicitud.informacion.datos_familiares ? {",
        "\t\t\t\t\ttutor: $data.solicitud.informacion.datos_familiares.tutor !== undefined ? $data.solicitud.informacion.datos_familiares.tutor : null,",
        "\t\t\t\t\tprofesion: $data.solicitud.informacion.datos_familiares.profesion !== undefined ? $data.solicitud.informacion.datos_familiares.profesion : null,",
        "\t\t\t\t\ttelefono: $data.solicitud.informacion.datos_familiares.telefono !== undefined ? $data.solicitud.informacion.datos_familiares.telefono : null",
        "\t\t\t\t} : null",
        "\t\t\t} : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
