      ],
      "exposed" : false
    },
    "cancelOutput" : {
      "type" : "json",
      "value" : [
        "{",
        "    \"periodoInput\": {",
        "        \"nombre\": \"\",",
        "        \"fecha_inicio\": null,",
        "        \"fecha_fin\": null,",
        "        \"activo\": false",
        "    },",
