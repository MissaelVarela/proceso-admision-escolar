    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.periodo ){",
        "\treturn {",
        "\t\t//map periodo variable to expected task contract input",
        "\t\tperiodoInput: {",
        "\t\t\tnombre: $data.periodo.nombre !== undefined ? $data.periodo.nombre : null,",
        "\t\t\tfecha_inicio: $data.periodo.fecha_inicio !== undefined ? $data.periodo.fecha_inicio : null,",
        "\t\t\tfecha_fin: $data.periodo.fecha_fin !== undefined ? $data.periodo.fecha_fin : null,",
        "\t\t\tactivo: $data.periodo.activo !== undefined ? $data.periodo.activo : null",
        "\t\t},",
        "\t\tcancelInput: false",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
